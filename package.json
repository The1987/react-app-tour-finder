    "aws-sdk": "^2.282.1",
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "connect-mongo": "^2.0.1",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "firebase": "^5.3.1",
    "google-maps-react": "^2.0.2",
    "invariant": "^2.2.4",
    "mongoose": "^4.11.7",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.7",
